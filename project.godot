; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Creature Addon"
run/main_scene="res://WallCritters.tscn"
config/icon="res://addons/creature_maker/icon.png"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "res://addons/creature_maker/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true
2d/default_gravity=980

[rendering]

environment/default_clear_color=Color( 1, 1, 1, 1 )
environment/default_environment="res://default_env.tres"
